<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        style="@style/top"
        android:text="设置中心" />

    <com.ycc.ui.EntryLayout
        xmlns:Entry="http://schemas.android.com/apk/res/com.ycc.bodyguard"
        android:id="@+id/el_setting_update"
        Entry:belowff="自动更新已关闭"
        Entry:belowon="自动更新已开启"
        Entry:titleName="是否开启自动更新"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.ycc.ui.EntryLayout>

    <com.ycc.ui.EntryLayout
        xmlns:Entry="http://schemas.android.com/apk/res/com.ycc.bodyguard"
        android:id="@+id/el"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        Entry:belowff="已关闭"
        Entry:belowon="已开启"
        Entry:titleName="是否开启来电显示">

    </com.ycc.ui.EntryLayout>

    <com.ycc.ui.EntryLayout
        xmlns:Entry="http://schemas.android.com/apk/res/com.ycc.bodyguard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        Entry:belowff="自动更新已关闭"
        Entry:belowon="自动更新已开启"
        Entry:titleName="是否开启来电显示">
    </com.ycc.ui.EntryLayout>
</LinearLayout>